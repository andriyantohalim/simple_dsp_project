
#
# Clock Configuration
#
# CONFIG_CLOCK_64_MHz is not set
CONFIG_CLOCK_32_MHz=y
# CONFIG_CLOCK_16_MHz is not set
# end of Clock Configuration

CONFIG_ADC_ENABLE=y

#
# ADC Configuration
#
CONFIG_ADC_CH0_ENABLE=y

#
# ADC CH0 Configuration
#
CONFIG_ADC_CH0_OFFSET=102
# end of ADC CH0 Configuration

CONFIG_ADC_CH1_ENABLE=y
CONFIG_ADC_CH2_ENABLE=y
# end of ADC Configuration

CONFIG_PWM_ENABLE=y

#
# PWM Configuration
#
CONFIG_PWM0_ENABLE=y

#
# PWM0 Configuration
#
CONFIG_PWM0_PERIOD=1024
CONFIG_PWM0_COMPARE=1024
# end of PWM0 Configuration

# CONFIG_PWM1_ENABLE is not set
# CONFIG_PWM2_ENABLE is not set
# end of PWM Configuration

CONFIG_DSP_ENABLE=y

#
# DSP Configuration
#
CONFIG_DSP_PI_CTRL_ENABLE=y

#
# PI Controller Configuration
#
CONFIG_PI_CTRL_KP=199
CONFIG_PI_CTRL_KI=20
CONFIG_PI_CTRL_SCALING="0.2f"
# end of PI Controller Configuration
# end of DSP Configuration
